{
	"globals": {
		"process": false,
		"after": false,
		"afterEach": false,
		"before": false,
		"beforeEach": false,
		"describe": false,
		"it": false,
		"require": false,
		"console": false,
		"window": false,
		"setTimeout": false,
		document: false
	},
	"parser": "babel-eslint",
	"rules": {
		"array-bracket-spacing": 2,
		"brace-style": [2, "stroustrup", {"allowSingleLine": true}],
		"camelcase": 2,
		"comma-dangle": 0,
		"comma-spacing": [2, {"before": false, "after": true}],
		"comma-style": [2, "last"],
		"computed-property-spacing": 2,
		"consistent-this": [2, "that"],
		"curly": 0,
		"eol-last": 2,
		"eqeqeq": [2, "allow-null"],
		"guard-for-in": 0,
		"indent": [1, "tab", {
			"SwitchCase": 1
		}],
		"key-spacing": 0,
		"new-cap": [2, {"capIsNew": false, "newIsCap": true}],
		"new-parens": 0,
		"no-bitwise": 0,
		"no-cond-assign": 2,
		"no-console": 1,
		"no-constant-condition": 2,
		"no-debugger": 2,
		"no-delete-var": 2,
		"no-dupe-keys": 2,
		"no-duplicate-case": 2,
		"no-empty-character-class": 2,
		"no-eval": 2,
		"no-ex-assign": 2,
		"no-extend-native": 2,
		"no-extra-parens": 0,
		"no-floating-decimal": 0,
		"no-func-assign": 2,
		"no-implied-eval": 2,
		"no-inner-declarations": 2,
		"no-invalid-regexp": 2,
		"no-irregular-whitespace": 2,
		"no-iterator": 2,
		"no-labels": 0,
		"no-lonely-if": 2,
		"no-loop-func": 2,
		"no-mixed-spaces-and-tabs": 2,
		"no-native-reassign": 2,
		"no-negated-in-lhs": 2,
		"no-new-func": 2,
		"no-new-object": 2,
		"no-new-wrappers": 2,
		"no-obj-calls": 2,
		"no-octal": 2,
		"no-plusplus": 0,
		"no-proto": 2,
		"no-redeclare": 2,
		"no-return-assign": 2,
		"no-self-compare": 2,
		"no-sequences": 2,
		"no-shadow": 0,
		"no-spaced-func": 2,
		"no-sparse-arrays": 2,
		"no-throw-literal": 2,
		"no-trailing-spaces": 2,
		"no-undef": 2,
		"no-undef-init": 2,
		"no-undefined": 0,
		"no-underscore-dangle": 0,
		"no-unreachable": 2,
		"no-unused-vars": [1, {"vars": "all", "args": "none"}],
		"no-use-before-define": [2, "nofunc"],
		"no-var": 2,
		"no-warning-comments": [0, {"terms": ["todo", "fixme"]}],
		"object-curly-spacing": 2,
		"quote-props": [0, "as-needed"],
		"quotes": [2, "single", "avoid-escape"],
		"radix": 2,
		"semi": [2, "always"],
		"semi-spacing": [2, {"before": false, "after": true}],
		"keyword-spacing": [2, "always"],
		"space-before-blocks": [2, "always"],
		"space-before-function-paren": [2, "never"],
		"space-in-parens": [0, "never"],
		"space-infix-ops": 2,
		"space-unary-ops": [2, {"words": true, "nonwords": false}],
		"strict": [0, "never"],
		"use-isnan": 2,
		"valid-typeof": 2,
		"wrap-iife": [2, "inside"],
		"yoda": 0,
		// Node.js specific
		"handle-callback-err": [2, "^(err|error)$"],
		"no-mixed-requires": 2,
		"no-new-require": 2,
		"no-path-concat": 2,
		// React
		// Not needed. "react/display-name": 2,
		"react/jsx-boolean-value": 0,
		"react/jsx-no-undef": 2,
		"react/jsx-sort-props": 0,
		"react/jsx-sort-prop-types": 0,
		"react/jsx-uses-react": 2,
		"react/jsx-uses-vars": 2,
		"react/no-did-mount-set-state": 2,
		"react/no-did-update-set-state": 2,
		"react/no-multi-comp": 2,
		"react/no-unknown-property": 2,
		"react/prop-types": 2,
		"react/react-in-jsx-scope": 2,
		"react/self-closing-comp": 2,
		// Not ready yet. "react/sort-comp": 2,
		"react/wrap-multilines": 2,
		// Import
		"import/no-unresolved": [0, {"commonjs": false, "amd": false}],
		"import/named": 2,
		"import/namespace": 2,
		"import/default": 2,
		"import/export": 2,
	},
	"plugins": [
		"import",
		"react"
	],
	"ecmaFeatures": {
		"jsx": true
	},
	"settings": {
		"import/ignore": [
			"node_modules",
			"\\.json$"
		],
		"import/parser": "babel-eslint",
		"import/resolve": {
			"extensions": [
				".js",
				".jsx",
				".json"
			],
			"resolve": [
				"/var/www/html/SSS/web/src"
			],
			"moduleDirectory": [
				"node_modules",
				"/var/www/html/SSS/web/src"
			]
		}
	}
}
